{
  "name": "jeopardybot",
  "version": "0.9.0",
  "description": "A bot for jeopardy",
  "main": "lib/index.js",
  "private": true,
  "scripts": {
    "postinstall": "npm run build",
    "build": "babel src --out-dir lib --copy-files && npm run admin",
    "watch": "npm run watch:build & npm run watch:browserify",
    "watch:build": "babel --watch src --out-dir lib --copy-files",
    "watch:browserify": "watchify lib/admin/index.js --outfile lib/admin/index.bundle.js -d",
    "admin": "npm run admin:browserify",
    "admin:browserify": "browserify lib/admin/index.js --outfile lib/admin/index.bundle.js",
    "start": "node lib/index",
    "dev": "nodemon --exec babel-node src/index",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src"
  },
  "author": "Jordan Gensler <jordangens@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "adaro": "^1.0.0",
    "async": "^1.5.0",
    "aws-sdk": "^2.2.11",
    "babel": "^5.8.23",
    "babel-runtime": "^5.8.25",
    "basic-auth-connect": "^1.0.0",
    "body-parser": "^1.14.1",
    "browserify": "^12.0.1",
    "cheerio": "^0.19.0",
    "decamelize": "^1.1.1",
    "dehumanize-date": "^2.1.0",
    "express": "^4.13.3",
    "express-remove-route": "^0.1.1",
    "express-restify-mongoose": "^2.3.1",
    "form-data": "^0.2.0",
    "get-stream": "^1.1.0",
    "history": "^1.17.0",
    "html-entities": "^1.2.0",
    "imagemin": "^3.2.0",
    "imagemin-pngquant": "^4.2.0",
    "images": "^2.1.8",
    "imgur": "^0.1.6",
    "lockfile": "^1.0.1",
    "moment": "^2.10.6",
    "mongoose": "^4.1.9",
    "natural": "^0.2.1",
    "node-fetch": "^1.3.3",
    "numeral": "^1.5.3",
    "pageres": "^2.1.0",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-mdl": "^1.0.2",
    "react-router": "^1.0.3",
    "require-dir": "^0.3.0",
    "slack-client": "^1.4.1",
    "string-to-number": "^0.1.1",
    "striptags": "^2.0.3",
    "temp": "^0.8.3",
    "unidecode": "^0.1.7"
  },
  "engines": {
    "node": "0.12"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.6",
    "eslint": "^1.6.0",
    "eslint-config-airbnb": "^1.0.2",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.1",
    "nodemon": "^1.7.1",
    "watchify": "^3.6.1"
  }
}
